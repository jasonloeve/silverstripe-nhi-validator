{
    "name": "jasonloeve/silverstripe-nhi-validator",
    "description": "NHI validator field for Silverstripe CMS.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "NHI Number",
        "NHI Validator",
        "New Zealand National Health Index Validation",
        "NZ Ministry of Health"
    ],
    "authors": [
        {
            "name": "Jason Loeve",
            "email": "jasonloeve@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/jasonloeve/nhi-validation-poc/issues"
    },
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "JasonLoeve\\NHIValidator\\": "src/",
            "JasonLoeve\\NHIValidator\\Tests\\": "tests/php/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
